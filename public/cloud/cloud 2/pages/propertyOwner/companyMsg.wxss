/* pages/authen/propertyOwner/companyMsg.wxss */
.wrap{
    padding: 0 30rpx;
}
.col-label {
    width: 285rpx;
}

.require {
    color: #EC0000;
}
.form-item{
    border-bottom: 2rpx solid #E1E1E1;
}
.submit{
    margin-top: 120rpx;
}
.btn{
    height: 88rpx;
    width: 100%;
    background: #04A7A9;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50rpx;
    box-sizing: border-box;
}

.uploadT{
    display: block;
    font-size:30rpx;
    font-weight: bold;
    color: #333;
    padding: 44rpx 0 30rpx 0;
}
.uploadTip{
    display: block;
    font-size:28rpx;
    color: #999
}
.upImg{
    background: #f8f8f8;
    padding:40rpx 0;
    margin: 32rpx 0 120rpx 0; 
}
.clickUpload image{
    width: 60rpx;
    height: 60rpx;
    margin: 0 auto;
}
.clickUpload text{
    text-align: center;
    font-size:26rpx;
    color: #04a7a9;
    display: block;
    margin-top: 24rpx;
}
.submit .btn{
    width: 630rpx;
    height: 88rpx;
    line-height: 88rpx;
    color: #fff;
    font-size: 30rpx;
    font-weight: bold;
    border-radius: 300rpx;
    background: #04a7a9;
    display: block;
    padding: 0;
    margin-bottom: 90rpx;
}